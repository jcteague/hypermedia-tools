@model object

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <title>title</title>
        <link href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/application.css")" rel="stylesheet" type="text/css" />
@*        <script src="@Url.Content("~/Scripts/json2.js")" type="text/javascript"> </script>*@
        <script src="@Url.Content("~/Scripts/jQuery/jquery-1.7.2.min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/Underscore/underscore-min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/Backbone/backbone-min.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/handlerbars/handlebars-1.0.0.beta.6.js")" type="text/javascript"> </script>
        <script src="@Url.Content("~/Scripts/app.js")" type="text/javascript"> </script>
        
    </head>
    <body>
        <div id="content" class="container">
              
        </div>
    </body>
    <script type="text/x-handlebars-template" id="link-template">
      <a href="{{href}}" rel="{{rel}}" class="{{name}}">{{prompt}}</a>
    </script>
    <script type="text/x-handlebars-template" id="form-field-template">
      <label for="{{name}}">{{prompt}}</label>
      <input type="text" id="{{Name}}" />
        
    </script>
    <script type="text/x-handlebars-template" id="resource-template">
      <form class="form-horizontal">
        {{#each Data}}
        <div class="control-group">
          <label class="control-label" for="inputPassword">{{prompt}}</label>
          <div class="controls">
            <span>{{value}}</span>
          </div>
        </div>
        
        {{/each}}
      </form>
        
    </script>

</html>